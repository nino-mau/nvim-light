{
  "console log": {
    "prefix": "clg",
    "body": ["console.log(${1})"]
  },
  "console error": {
    "prefix": "cle",
    "body": ["console.error('[${1}]')"]
  },
  "async named function": {
    "prefix": "fae",
    "body": ["export async function ${1:function}() {", " ${2}", "};"]
  },
  "arrow function": {
    "prefix": "cf",
    "body": ["const ${1:name} = () => {", " ${2}", "};"]
  },
  "async anonymous function": {
    "prefix": "fano",
    "body": ["(async () => {", " ${1}", "} )();"]
  },

  "fetch request": {
    "prefix": "fetch",
    "body": [
      "const response = await fetch('${1:/api/example}', {",
      "  method: '${2:GET}',",
      "  headers: {",
      "    'Content-Type': 'application/json'",
      "  },",
      "  body: ${3:JSON.stringify(${4:data})}",
      "});",
      "if (!response.ok) {",
      "  throw new Error('Request failed');",
      "}",
      "const ${5:result} = await response.json();"
    ]
  },
  "exportType": {
    "body": ["export type ${1:first} = {${2:second}}"],
    "prefix": "exptp"
  }
}
